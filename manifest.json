{
  "name": "Library search",
  "description": "find a book from a library on bookmeter.com",
  "version": "2.1",
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_title": "Find books from a library"
  },
  "manifest_version": 2
}